<div v-else-if=\"type=='info'\">\r\n                        <el-form-item class=\"input\" label=\"车位状态\" prop=\"cheweiZhuangtaiValue\">\r\n                        <el-input v-model=\"ruleForm.cheweiZhuangtaiValue\"\r\n                            placeholder=\"车位状态\" readonly></el-input>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n               <el-col :span=\"12\">\r\n                   <el-form-item class=\"input\" v-if=\"type!='info'\"  label=\"金额/小时\" prop=\"cheweiNewMoney\">\r\n                       <el-input v-model=\"ruleForm.cheweiNewMoney\"\r\n                                 placeholder=\"金额/小时\" clearable  :readonly=\"ro.cheweiNewMoney\"></el-input>\r\n                   </el-form-item>\r\n                   <div v-else-if=\"type=='info'\">\r\n                       <el-form-item class=\"input\" label=\"金额/小时\" prop=\"cheweiNewMoney\">\r\n                           <el-input v-model=\"ruleForm.cheweiNewMoney\"\r\n                                     placeholder=\"金额/小时\" readonly></el-input>\r\n                       </el-form-item>\r\n                   </div>\r\n               </el-col>\r\n                <el-col :span=\"24\">\r\n                    <el-form-item v-if=\"type!='info' && !ro.cheweiContent\"  label=\"车位介绍\" prop=\"cheweiContent\">\r\n                        <editor style=\"min-width: 200px; max-width: 600px;\"\r\n                                v-model=\"ruleForm.cheweiContent\"\r\n                                class=\"editor\"\r\n                                action=\"file/upload\">\r\n                        </editor>\r\n                    </el-form-item>\r\n                    <div v-else-if=\"type=='info'\">\r\n                        <el-form-item v-if=\"ruleForm.cheweiContent\" label=\"车位介绍\" prop=\"cheweiContent\">\r\n                            <span v-html=\"ruleForm.cheweiContent\"></span>\r\n                        </el-form-item>\r\n                    </div>\r\n                </el-col>\r\n            </el-row>\r\n            <el-form-item class=\"btn\">\r\n                <el-button v-if=\"type!='info'\" type=\"primary\" class=\"btn-success\" @click=\"onSubmit\">提交</el-button>\r\n                <el-button v-if=\"type!='info'\" class=\"btn-close\" @click=\"back()\">取消</el-button>\r\n                <el-button v-if=\"type=='info'\" class=\"btn-close\" @click=\"back()\">返回</el-button>\r\n            </el-form-item>\r\n        </el-form>\r\n    </div>\r\n</template>\r\n<script>\r\n    import styleJs from \"../../../utils/style.js\";\r\n    // 数字，邮件，手机，url，身份证校验\r\n    import { isNumber,isIntNumer,isEmail,isPhone, isMobile,isURL,checkIdCard } from \"@/utils/validate\";\r\n    import utilsJs, {getYearFormat,getMonthFormat,getDateFormat,getDatetimeFormat} from \"../../../utils/utils.js\";\r\n    export default {\r\n        data() {\r\n            return {\r\n                addEditForm:null,\r\n                id: '',\r\n                type: '',\r\n                sessionTable : \"\",//登录账户所在表名\r\n                role : \"\",//权限\r\n                userId:\"\",//当前登录人的id\r\n                ro:{\r\n                    cheweiName: false,\r\n                    cheweiUuidNumber: true,\r\n                    cheweiPhoto: false,\r\n                    cheweiAddress: false,\r\n                    cheweiTypes: false,\r\n                    cheweiZhuangtaiTypes: false,\r\n                    cheweiNewMoney: false,\r\n                    cheweiContent: false,\r\n                    cheweiDelete: false,\r\n                    insertTime: false,\r\n                },\r\n                ruleForm: {\r\n                    cheweiName: '',\r\n                    cheweiUuidNumber: new Date().getTime(),\r\n                    cheweiPhoto: '',\r\n                    cheweiAddress: '',\r\n                    cheweiTypes: '',\r\n                    cheweiZhuangtaiTypes: '',\r\n                    cheweiNewMoney: '',\r\n                    cheweiContent: '',\r\n                    cheweiDelete: '',\r\n                    insertTime: '',\r\n                },\r\n                cheweiTypesOptions : [],\r\n                cheweiZhuangtaiTypesOptions : [],\r\n                rules: {\r\n                   cheweiName: [\r\n                              { required: true, message: '车位名称不能为空', trigger: 'blur' },\r\n                          ],\r\n                   cheweiUuidNumber: [\r\n                              { required: true, message: '车位编号不能为空', trigger: 'blur' },\r\n                          ],\r\n                   cheweiPhoto: [\r\n                              { required: true, message: '车位照片不能为空', trigger: 'blur' },\r\n                          ],\r\n                   cheweiAddress: [\r\n                              { required: true, message: '车位地点不能为空', trigger: 'blur' },\r\n                          ],\r\n                   cheweiTypes: [\r\n                              { required: true, message: '车位类型不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   cheweiZhuangtaiTypes: [\r\n                              { required: true, message: '车位状态不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]*$/,\r\n                                  message: '只允许输入整数',\r\n                                  trigger: 'blur'\r\n                              }\r\n                          ],\r\n                   cheweiNewMoney: [\r\n                              { required: true, message: '金额/小时不能为空', trigger: 'blur' },\r\n                              {  pattern: /^[0-9]{0,6}(\\.[0-9]{1,2})?$/,\r\n                                  message: '只允许输入整数6位,小数2位的数字',\r\n                                  trigger: 'blur'\r\n                              }\r\n              